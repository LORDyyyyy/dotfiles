    "fileNestingUpdater.promptOnAutoUpdate": false,
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "liveServer.settings.ChromeDebuggingAttachment": false,
    "projectManager.git.baseFolders": ["/home/lordy/Github"],
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "scminput": false,
        "markdown": false
    },
    "cSpell.userWords": ["lordy"],
    "cSpell.ignorePaths": [
        "node_modules/",
        "vscode-extension",
        ".vscode",
        ".vscode-insiders",
        "package.json",
        "package-lock.json",
        "yarn.lock",
        ".git/",
        ".gitignore"
