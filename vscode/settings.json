    // Settings Cycler Extension
    "settings.cycle": [
        {
            "id": "clean-setup",
            "values": [
                {
                    "workbench.activityBar.location": "hidden",
                    "editor.glyphMargin": false,
                    "breadcrumbs.enabled": false,
                    "workbench.editor.editorActionsLocation": "hidden",
                    "workbench.statusBar.visible": false,
                    "scm.diffDecorations": "none"
                },
                {
                    "workbench.activityBar.location": "default",
                    "editor.glyphMargin": true,
                    "breadcrumbs.enabled": true,
                    "workbench.editor.editorActionsLocation": "default",
                    "workbench.statusBar.visible": true,
                    "scm.diffDecorations": "gutter"
                }
            ]
        }
    ],

    // Include "-" and "$" in word selection.
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",

    // Language Specific
    "html.format.wrapLineLength": 80,
    "html.format.wrapAttributes": "preserve",
    // Extentions Specific
    "prettier.tabWidth": 4,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",

    // Editor Actions
    "editor.codeActionsOnSave": {},
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "files.autoSaveWhenNoErrors": true,
    "files.exclude": {
        "package-lock.json": true,
        "yarn.lock": true
    },
    "files.defaultLanguage": "",
    // Configure file associations for languages ​​(e.g. " *.extension ": "html" ). These associations take precedence over the default associations for installed languages.
    "files.associations": {},
    // Controls whether to use .gitignore files by default or .ignore files when searching in a new workspace.
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/vendor/{[^l],?[^ai]}*": true,
        "**/public/{[^i],?[^n]}*": true,
        "**/dist": true,
        "**/_ide_helper.php": true,
        "**/composer.lock": true,
        "**/package-lock.json": true,
        "storage": true,
        ".phpunit.result.cache": true
    },

