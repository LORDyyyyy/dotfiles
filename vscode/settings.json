{
    // Profile: Default

    "window.zoomLevel": 0,

    "workbench.startupEditor": "none",

    // Font
    "editor.fontSize": 18,
    "editor.fontFamily": "JetBrains Mono, Noto Nerd Emoji, 'DejaVu Sans Mono', monospace",

    // Appearances
    "workbench.colorTheme": "Dark+ Tweaked",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "none",

    "editor.lineHeight": 1.5,
    "editor.lineNumbers": "relative",
    // Controls how the editor highlights the current line.
    "editor.renderLineHighlight": "line",

    // The left padding in the line number columns. Used for images and other extensions.
    "editor.glyphMargin": true,
    "editor.stickyScroll.enabled": false,
    "editor.minimap.enabled": false,
    // Controls whether the cursor is hidden in the overview ruler.
    "editor.hideCursorInOverviewRuler": true,

    // Controls whether the editor highlights occurrences of semantic symbols.
    "editor.occurrencesHighlight": "off",

    "editor.autoClosingBrackets": "languageDefined",

    "editor.cursorSmoothCaretAnimation": "on",

    "editor.guides.indentation": false,
    "editor.padding.top": 3,

    // When you open a file, `editor.tabSize` and `editor.insertSpaces` will be detected based on the file contents.
    "editor.detectIndentation": true,

    // Controls whether the 'Enter' key accepts suggestions in addition to the 'Tab' key.
    "editor.acceptSuggestionOnEnter": "on",

    // Controls whether snippets are displayed with other suggestions and how they are sorted.
    "editor.snippetSuggestions": "bottom",

    // Controls how whitespace is displayed in the editor.
    "editor.renderWhitespace": "none",

    "editor.accessibilitySupport": "off",

    "breadcrumbs.enabled": true,

    "workbench.layoutControl.enabled": false,
    "workbench.layoutControl.type": "menu",

    "workbench.statusBar.visible": true,

    "workbench.editor.wrapTabs": true,
    // When enabled, the watermark hint will be displayed when no editors are open.
    "workbench.tips.enabled": false,
    // The run button (and other buttons) in the top left.
    "workbench.editor.editorActionsLocation": "default",
    "workbench.activityBar.location": "default",

    "window.title": " ",
    "window.commandCenter": false,
    // For the Default Profile Only.
    "window.experimentalControlOverlay": false,
    "window.menuBarVisibility": "toggle", // title frame menu bar
    "window.customTitleBarVisibility": "auto",
    "window.titleBarStyle": "custom",

    // Git
    "scm.diffDecorations": "gutter",

    "workbench.tree.indent": 10,
    "workbench.tree.renderIndentGuides": "none",
    // Controls whether the Explorer automatically shows and selects the file contents when opening a file.
    "explorer.autoReveal": false,
    // Controls whether grouping parent folder with the only single child folder.
    "explorer.compactFolders": false,
    "explorer.decorations.colors": false, // Example: when a file contains an error, change the file color to red or not.
    "explorer.confirmDelete": false,
